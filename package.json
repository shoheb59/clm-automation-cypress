  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.32.0",
    "allure-cypress": "^3.0.5",
    "cypress": "^13.12.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-if": "^1.13.1",
    "cypress-iframe": "^1.0.1",
